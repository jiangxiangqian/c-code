//输入两个整数，求他们的和和积.
#include<stdio.h>
int main()
{
	int a, b, sum, s;
	printf("输入两个整数:");
	scanf_s("%d%d", &a, &b);
	sum = a + b;
	s = a * b;
	printf("sum=%d,s=%d", sum, s);
	return 0;
}

